
public void printd(number d)
public number factorial(integer n)
public integer atoi(string s)


public integer entry (integer argc, string *argv, string *envp) {
	number res

	if (argc = 1) then {
		res := factorial(5)

		printd(res)

		entry := 0
	}
	else if (argc = 2) then {
		res := factorial(atoi(argv[1]))

		printd(res)

		entry := 0
	}
	else {
		entry := 1
	}

};