
public void printd(number d)
public integer rand()
public number atoi()


public integer entry (integer argc, string *argv, string *envp) {

	if (argc = 2) then {
		integer i, n := atoi(argv[1])

		if(n > 0) then {
			number res := 0

			for i in 1 upto n {
				res := res + rand()
			}

			res := res / n

			printd(res)

			entry := 0
		}
		else {
			entry := 1
		}
		
	}
	else {
		entry := 1
	}

};