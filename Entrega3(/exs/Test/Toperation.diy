public void prints(string s)
public void printi(integer i)

public void entry (integer argc, string *argv, string *envp) {
  printi(1 + 3) == +
  printi(3 - 1) == -
  printi(2 * 3) == *
  printi(4 / 2) == /
  printi(3 % 2) == %
  printi(-2)    == -a
  prints(" ")
  printi(1 < 3) == <
  printi(1 > 3) == >
  printi(1 = 3) == =
  printi(1 = 1)
  printi(1 & 3) == &
  printi(1 & 0)
  prints(" ")
  printi(0 | 0) == |
  printi(1 | 0)
  printi(1 >= 1) == GE
  printi(1 >= 2)
  printi(1 <= 0) == LE
  printi(1 <= 1)
  prints(" ")
  printi(1 <> 0)  == NE
  printi(1 <> 1)
  printi(~1) == ~
  printi(~0)
  prints("\n42621-2 100110 011001 1001 <- expected\n")

};

== expected 42621-2 100110 011001 1001
